<!DOCTYPE html>
<html>
<head>
    <% include ../partials/header.ejs %>
</head>
<body>

<div class="container">
    <h1>Page</h1>
    <form action="">
        <label for="first_name" >First Name</label>
        <input type="text" name="first_name" id="first_name" value="alex17" />
        <a href="javascript:void(0)" class="btn btn-primary" id="btnSubmit">Submit</a>
    </form>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/superagent/4.1.0/superagent.min.js"></script>
    <script>

        document.querySelector('#btnSubmit').onclick = () => {

	        superagent
		        .post('/login')
		        .set("Content-Type", "application/x-www-form-urlencoded")
		        .send({ username: 'Manny', species: 'cat' }) // sends a JSON post body
		        .end((err, res) => {
			        console.log("res: ", res.body)
		     });
        };

    </script>
</div>
</body>
</html>